{
  "name": "gql-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@craco/craco": "^6.3.0",
    "@nivo/core": "^0.74.0",
    "@nivo/line": "^0.74.0",
    "@nivo/scales": "^0.74.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.28",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/recharts": "^1.8.21",
    "date-fns": "^2.25.0",
    "eslint-plugin-graphql": "^4.0.0",
    "graphql": "^15.6.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-select": "^5.1.0",
    "recharts": "^2.1.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "get-gql-schema": "npx apollo client:download-schema schema.graphql",
    "get-json-schema": "npx apollo client:download-schema schema.json",
    "get-schema": "npm run get-gql-schema && npm run get-json-schema",
    "generate-types": "npx apollo client:codegen --target=typescript --localSchemaFile=./schema.graphql --no-addTypename ./types"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "share-config"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.175",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "apollo": "^2.33.6",
    "typescript": "^4.4.3"
  }
}
